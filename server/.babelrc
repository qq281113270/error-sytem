{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": true
        }
      },
      "stage-0"
    ]
  ],
  "plugins": [
    "transform-es2015-modules-commonjs",
    "@babel/plugin-transform-runtime",
    [
      "@babel/plugin-transform-regenerator",
      {
        "asyncGenerators": false,
        "generators": false,
        "async": false
      }
    ]
  ]
}
