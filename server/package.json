  "scripts": {
    "//注释1": "webpack编译node源码",
    "build:dev": "cross-env NODE_ENV=development    babel-node ./webpack/index.js",
    "build:prd": "cross-env NODE_ENV=production     babel-node ./webpack/index.js",
    "//注释2": "webpack编译node源码，并且启动node程序",
    "start": "npm run build:dev&npm run dev:pm2",
    "//注释3": "用pm2启动node程序",
    "dev": "npm run dev:pm2",
    "dev:pm2": "cross-env NODE_ENV=development  pm2 start ./dist/index.js --watch",
    "test:pm2": "cross-env NODE_ENV=production  pm2 start ./dist/index.js --watch",
    "prd:pm2": "cross-env NODE_ENV=production   pm2 start ./dist/index.js --watch",
    "//注释4": "用nodemon启动node程序",
    "dev:nodemon": "nodemon  --exec cross-env NODE_ENV=development   babel-node ./dist index.js",
    "test:nodemon": "nodemon --exec cross-env NODE_ENV=production    babel-node ./dist index.js",
    "prd:nodemon": "nodemon  --exec cross-env NODE_ENV=production    babel-node ./dist index.js",
    "//注释5": "bable转义,一般用webpack转义,该指令只用来调试",
    "babel": " babel ./app --watch --out-dir ./dist",
    "//注释6": "建议不用dev:source这个指令,这个指令启动服务有可能会报错",
    "dev:source": "nodemon  --exec cross-env  NODE_ENV=development  babel-node ./app index.js",
    "testflie": "node  ./test.js",
    "//注释7": "编译dll文件",
    "dll": "webpack   --progress --config ./webpack/config/webpack.dll.config.js",
    "stats": "webpack --progress --config ./webpack/config/index.js  --profile --json > stats.json"
  },
